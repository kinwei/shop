<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><!doctype html><html><head><meta charset="UTF-8"><title>产品分类</title><link type="text/css" rel="stylesheet" href="../style/reset.css"><link type="text/css" rel="stylesheet" href="../style/main.css"><link type="text/css" rel="stylesheet" href="../style/shopCar.css"><link type="text/css" rel="stylesheet" href="../style/detail.css"><%@include file="/public/head.jspf"%></head>	<body>	<div class="headerBat">    	<div class="topBar">        	<div class="comWidth">            	<div class="leftArea">            		<a href="#" class="collection">收藏地摊</a>                </div>                <div class="rightArea">                	 <c:choose>                <c:when test="${sessionScope.user == null}">                	欢迎来到小地摊 <a href="login.jsp">[登录]</a>                    <a href="register.jsp">[注册]</a>                 </c:when>                 <c:otherwise>                 	欢迎${sessionScope.user.name}来到小地摊                 	<a href="user_logout.action">安全退出</a>                 </c:otherwise>                 </c:choose>                </div>            </div>        </div>                <div class="logoBar">        	<div class="comWidth">            	<div class="logo fl">                	<a href="../index3.jsp"><img src="../images/logo.jpg" alt="shop"></a>                </div>                <div class="stepBox fr">                	<div class="step">                    	                    </div>                    <ul class="step_text">                    	<li class="s01 active">我的购物车</li>                        <li class="s02 active">填写核对订单</li>                        <li class="s03">订单提交成功</li>                    </ul>                </div>            </div>        </div>       <form name="fsave" action="forder_save.action" method="post">      <div class="shopCarInfo comWidth">      	<div class="shopCar_item">        	<h3 class="shopCar_title">收货信息</h3>            <div class="shopCar_content padding_shop">            	<ul class="shopCar_list">                	                    <li><span class="shopCar_list_text"><em>*</em>区域邮编：</span><input name="post" class="input" type="text"  placeholder="如：252429"/></li>                    <li><span class="shopCar_list_text"><em>*</em>详细地址：</span><input name="address" class="input input_big" type="text"  placeholder="最多输入20个汉字"/></li>                    <li><span class="shopCar_list_text"><em>*</em>收货人：</span><input name="name" class="input"  type="text" value="${sessionScope.user.name}" placeholder="最多输入10个汉字"/></li>                    <li><span class="shopCar_list_text"><em>*</em>手机：</span><input name="phone"class="input" type="text" value="${sessionScope.user.phone}" placeholder="如：17308416757"/></li>                    <li><input type="button" class="confirm"></li>                </ul>            </div>        </div>       </form>        	<div class="shopCar_item">        	<h3 class="shopCar_title">支付方式</h3>            <div class="shopCar_content padding_shop">            	<ul class="shopCar_list">                	<li><input type="radio" class="radio" id="r1"><label for="r1">微信支付</label></li>                    <li><input type="radio" class="radio"id="r1"><label for="r1">微信支付</label></li>                </ul>            </div>        </div>                <div class="shopCar_item">        	<h3 class="shopCar_title">送货清单<a href="../shopCar.jsp" class="backCar">返回购物车修改</a></h3>            <div class="shopCar_content">            	<div class="cart_inner">                	<div class="cart_head clearfix">                    	<div class="cart_item item_name">商品名称</div>                        <div class="cart_item item_price">单价</div>                        <div class="cart_item item_price">返现</div>                        <div class="cart_item item_price">数量</div>                        <div class="cart_item item_sum">小计</div>                    </div>                  <c:forEach items="${sessionScope.forder.sorderSet}" var="sorder">                    <div class="cart_content clearfix">                    	<div class="cart_item item_name">                        	<div class="cart_shopInfo clearfix">                            	<img src="${shop}/image/${sorder.product.pic}"/>                                <div class="cart_shopInfo_content">                                	<p class="cart_link"><a href="${shop}/product_get.action?id=${sorder.product.id}">${sorder.name}</a></p>                                    <p class="cart_info">${sorder.product.remark}</p>                                </div>                            </div>                        </div>                        <div class="cart_item item_price">${sorder.price}</div>                        <div class="cart_item item_price">0</div>                        <div class="cart_item item_price">${sorder.number}</div>                        <div class="cart_item item_sum">￥${sorder.price*sorder.number}</div>                    </div>                 </c:forEach>                </div>             </div>           </div>                <div class="shopCar_item">        	<h3 class="shopCar_title">订单结算</h3>            <div class="shopCar_content padding_shop clearfix">            	<div class="cart_count fr">                	<div class="cart_rmb">                    	<i>总计：</i><span>￥${forder.total}</span>                    </div>                    <input type="button" value="提交订单" onclick="fsave.submit()" />                </div>            </div>        </div>      </div>   		       <div class="footer">    	<p></p>        <p></p>        <p><a href="#">地摊简介</a> <i>|</i> <a href="#">地摊公告</a><i>|</i><i>|</i> <a href="#">联系我</a><i>|</i> <a href="#">tel：17308416757</a> </p>        <p>blablablablablablablablablablablablablablablablablablablabla</p>            </div>        <script src="js/jquery-2.1.4.min.js"></script>    <script src="js/main.js"></script></body></html>