<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%><%String path = request.getContextPath();String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html><head><meta http-equiv="Content-Type" content="text/html; charset=gb2312"/><title>注册成功</title><script type="text/javascript"> function countDown(secs,surl){  //alert(surl);  var jumpTo = document.getElementById('jumpTo'); jumpTo.innerHTML=secs;  if(--secs>0){   setTimeout("countDown("+secs+",'"+surl+"')",1000);  } else {    location.href=surl;  } } </script></head><body><span id="jumpTo">5</span>秒后自动跳转到<a href="index3.jsp">主页</a><script type="text/javascript">countDown(5,'index3.jsp');</script> </body></html>